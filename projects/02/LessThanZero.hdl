/**
 * Computes whether two 16 bit buses are equal
 */

CHIP LessThanZero {
    IN a[16];  // 1-bit inputs
    OUT out;   // 1 if a == b else 0

    PARTS:
    Eq(a=a[15], b=true, out=out);
}